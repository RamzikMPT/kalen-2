<Page x:Class="WpfApp5.Stranvib"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp5"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Stranvib">

    <Grid HorizontalAlignment="Right" Width="800" >
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="0.2*"/>
        </Grid.RowDefinitions>
        
        <ListBox x:Name="SelectedList" d:ItemsSource="{d:SampleData ItemCount=5}"   HorizontalAlignment="Left" Width="800"/>
        <Button x:Name="Exit" Content="exit" HorizontalAlignment="Left"  Width="800" Click="Exit_Click" Grid.Row="1" />

    </Grid>
</Page>
